{
  "name": "movies-pwa",
  "version": "1.0.0",
  "description": "Project experiment with Nuxt.js, Workbox and IndexedDB",
  "author": "Muhammad Abdul Aziz <me@zizcode.web.id>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "node build/main.js",
